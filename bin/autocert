#!/usr/bin/env python3
# -*- coding: utf-8 -*-
'''
Convenience wrapper for running cli directly from source tree.
'''

import os
import sys

DIR = os.path.dirname(__file__)
sys.path.insert(0, os.path.abspath(os.path.join(DIR, '../autocert')))

from cli import main

if __name__ == '__main__':
    main()
