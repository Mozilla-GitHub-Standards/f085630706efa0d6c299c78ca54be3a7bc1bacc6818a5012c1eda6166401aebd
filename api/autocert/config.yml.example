logging:
    version: 1
    disable_existing_loggers: true
    root:
        level: NOTSET
        handlers: [console]
    loggers:
        api:
            level: WARNING
            handlers: [console]
            propagate: false
    handlers:
        console:
            class: logging.StreamHandler
            stream: ext://sys.stdout
            formatter: simple
            level: WARNING
    formatters:
        simple:
            format: '%(asctime)s - %(name)s - %(levelname)s - %(message)s'
        detail:
            format: '%(asctime)s - %(levelname)s - File: %(filename)s - %(funcName)s()
                - Line: %(lineno)d - %(message)s'
authorities:
    digicert:
        baseurl: https://www.digicert.com:443/services/v2
        apikey: 82_CHAR_APIKEY
        template:
            certificate:
                organization_units: [WebOps]
                server_platform:
                    id: 2 #Apache
                signature_hash: sha256
            organization:
                id: 147486
            payment_method: balance
            validity_years: 1
        headers:
            Content-Type: application/json
            User-Agent: auto-cert
destinations:
    zeus:
        baseurl: http://10.248.29.12:9070/api/tm/3.9
        apikey: 82_CHAR_APIKEY
csr:
    dirpath: /data/auto-cert
